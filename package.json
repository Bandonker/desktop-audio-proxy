{
  "name": "desktop-audio-proxy",
  "version": "1.1.7",
  "description": "A comprehensive media streaming solution for Tauri and Electron apps that bypasses CORS and WebKit codec issues. Supports audio (MP3, AAC, WAV) and video (MP4, M3U8/HLS, WebM).",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.esm.js",
      "require": "./dist/browser.cjs",
      "types": "./dist/browser.d.ts"
    },
    "./server": {
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs",
      "types": "./dist/react.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.cjs",
      "types": "./dist/vue.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run build && npm test",
    "lint": "eslint src --ext .ts && prettier --check src",
    "lint:fix": "eslint src --ext .ts --fix && prettier --write src",
    "prepublishOnly": "npm run build",
    "proxy:start": "node examples/standalone-server.js",
    "example": "npm run build && npm run proxy:start",
    "demo": "node demo/start-demo.js",
    "demo:cli": "node demo/cli-demo.js",
    "demo:serve": "cd demo && python3 -m http.server 8080",
    "demo:simple": "npm run build && npm run proxy:start"
  },
  "keywords": [
    "tauri",
    "electron",
    "audio",
    "proxy",
    "cors",
    "streaming",
    "webkit",
    "media",
    "react",
    "vue",
    "hooks",
    "composables",
    "typescript",
    "bandonker",
    "desktop-audio",
    "audio-proxy",
    "cors-bypass",
    "tauri-cors",
    "electron-audio",
    "tauri-audio",
    "electron-cors",
    "desktop-streaming",
    "webkit-codec",
    "audio-streaming",
    "video-streaming",
    "hls-streaming",
    "m3u8-playlist",
    "mp4-streaming",
    "cors-workaround",
    "podcast-streaming",
    "radio-streaming",
    "web-audio-api",
    "desktop-media",
    "cross-origin",
    "tauri-streaming",
    "electron-streaming"
  ],
  "author": {
    "name": "Bandonker",
    "url": "https://github.com/bandonker"
  },
  "contributors": [
    "Bandonker"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/bandonker"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bandonker/desktop-audio-proxy.git"
  },
  "homepage": "https://github.com/bandonker/desktop-audio-proxy#readme",
  "bugs": {
    "url": "https://github.com/bandonker/desktop-audio-proxy/issues"
  },
  "files": [
    "dist",
    "src",
    "demo",
    "examples",
    "assets",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "SECURITY.md",
    "TAURI_MIGRATION.md"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.11.1",
    "rollup": "^3.0.0",
    "ts-jest": "^29.4.0",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
